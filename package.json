{
	"name": "nextjs-quick-starter",
	"version": "0.1.0",
	"private": true,
	"description": "Modern Next.js 16 starter with React 19, Drizzle ORM, TanStack libraries, Vercel AI SDK, and best practices",
	"author": "abidjappie",
	"license": "MIT",
	"engines": {
		"node": ">=24.0.0",
		"pnpm": ">=8.0.0"
	},
	"scripts": {
		"dev": "next dev",
		"build": "pnpm db:migrate && next build",
		"start": "next start",
		"lint": "pnpm dlx @biomejs/biome check --write",
		"auth:generate": "pnpm dlx @better-auth/cli@latest generate",
		"db:generate": "drizzle-kit generate",
		"db:push": "drizzle-kit push",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio"
	},
	"dependencies": {
		"@ai-sdk/anthropic": "^2.0.44",
		"@ai-sdk/google": "^2.0.32",
		"@ai-sdk/openai": "^2.0.67",
		"@ai-sdk/react": "^2.0.93",
		"@libsql/client": "^0.15.8",
		"@next/env": "^16.0.3",
		"@tanstack/react-query": "^5.90.9",
		"@tanstack/react-query-devtools": "^4.42.0",
		"@tanstack/react-store": "^0.8.0",
		"ai": "^5.0.93",
		"better-auth": "^1.3.34",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"drizzle-kit": "^0.31.1",
		"drizzle-orm": "^0.44.2",
		"drizzle-zod": "^0.8.3",
		"lucide-react": "^0.553.0",
		"next": "16.0.3",
		"next-themes": "^0.4.6",
		"react": "19.2.0",
		"react-confetti": "^6.4.0",
		"react-dom": "19.2.0",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.4.0",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.2",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20",
		"@types/react": "^19.2.5",
		"@types/react-dom": "^19.2.3",
		"i18next-cli": "^1.24.8",
		"lefthook": "^2.0.4",
		"tailwindcss": "^4",
		"tsx": "^4.20.6",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5"
	}
